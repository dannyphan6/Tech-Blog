<div id="login-form">
    <form>
        <h1>Sign Up</h1>
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Username</label>
            <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="EX: johndoe">
        </div>
        <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Password</label>
            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="EX: password123">
        </div>
        <div class="mb-3">
            <button type="submit" class="btn btn-primary ml-3">Submit</button>
        </div>
    </form>
</div>